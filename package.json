{
  "name": "octobay-subgraph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create octobay/octobay --node https://api.thegraph.com/deploy/",
    "create-dev": "graph create octobay/octobay-dev --node https://api.thegraph.com/deploy/",
    "create-local": "graph create octobay/octobay --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy octobay/octobay --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-dev": "graph deploy octobay/octobay-dev subgraph-dev.yaml --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy octobay/octobay --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
